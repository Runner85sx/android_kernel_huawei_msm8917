/dts-v1/;
#include "huawei_msm8937_base.dtsi"
#include "lcdkit/lnd_l23_va/devkit_lcd.dtsi"
#include "lcdkit/default/devkit_lcd.dtsi"
#include "huawei_msm8937_lnd_l23_va/huawei.dtsi"
/ {
	model = "Huawei Technologies, Inc. LDN-LX3";
	compatible = "qcom,msm8937-pmi8937-qrd-sku2",
                 "qcom,msm8937-qrd", "qcom,msm8937", "qcom,qrd";
	qcom,board-id= <8200 0>;
	huawei,modem_id = <0xF8B00111 0x0>;
};
&mdss_dsi0_pll {
	/delete-property/ qcom,dsi-pll-ssc-en;
	/delete-property/ qcom,dsi-pll-ssc-mode;
};
&rpm_bus {
	rpm-regulator-ldoa6 {
		pm8937_l6: regulator-l6 {
			regulator-always-on;
		};
	};
	rpm-regulator-ldoa10 {
		pm8937_l10: regulator-l10 {
			regulator-always-on;
		};
	};
};
