/dts-v1/;
#include "huawei_msm8937_nopmi_base.dtsi"
#include "lcdkit/aum_al30_vb/devkit_lcd.dtsi"
#include "lcdkit/default/devkit_lcd.dtsi"
#include "huawei_msm8937_atu_al30b_va/huawei.dtsi"
/ {
        model = "Huawei Technologies, Inc. AUM-AL30";
        compatible = "qcom,msm8937-pmi8937-qrd-sku2",
                 "qcom,msm8937-qrd", "qcom,msm8937", "qcom,qrd";
        qcom,board-id= <8333 0>;
        huawei,modem_id = <0xF8FC012B 0x0>;
};
&rpm_bus {
        rpm-regulator-ldoa6 {
                pm8937_l6: regulator-l6 {
                        regulator-always-on;
                };
        };
        rpm-regulator-ldoa10 {
                pm8937_l10: regulator-l10 {
                        regulator-always-on;
                };
        };
};

